// mang nhap hang buoi sang 1
int nhaphangsang1()
{
  int a;
  cout << "Nhap so luong '247' cua ca ngay:  ";
  cin >> a;
  return a;
}
// mang nhap hang buoi toi 1
int nhaphangtoi1()
{
  int a1;
  cout << "Nhap so luong '247' cua buoi toi: ";
  cin >> a1;
  return a1;
}
